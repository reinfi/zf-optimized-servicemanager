{
  "name": "reinfi/zf-optimized-servicemanager",
  "type": "library",
  "description": "A Zend Framework module for optimizing service manager calls.",
  "keywords": [
    "zend",
    "servicemanager",
    "optimized"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Reinfi"
    }
  ],
  "require": {
    "php": ">=7.0",
    "zendframework/zend-servicemanager": "~2.7",
    "zendframework/zend-modulemanager": "~2.7",
    "zendframework/zend-mvc": "~2.7",
    "zendframework/zend-console": "~2.6",
    "reinfi/zf-dependency-injection": "^1.0",
    "nette/php-generator": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Reinfi\\OptimizedServiceManager\\": [
        "src/",
        "test/"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "satooshi/php-coveralls": "^1.0",
    "phpunit/phpcov": "^3.0"
  }
}
